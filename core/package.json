{
  "name": "social-housing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "truffle compile && vue-cli-service serve",
    "test": "truffle test",
    "ganache": "ganache-cli -i 3 -a 10",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "compile": "truffle compile",
    "deploy-kovan": "truffle compile && truffle migrate --network kovan --reset",
    "deploy-skale": "truffle compile && truffle migrate --network skale --reset",
    "bridge": "node bridge/job.js"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "core-js": "^2.6.5",
    "dialog-polyfill": "^0.5.0",
    "ethers": "^4.0.27",
    "openzeppelin-solidity": "^2.2.0",
    "truffle": "^5.0.0-beta.2",
    "truffle-hdwallet-provider": "^1.0.6",
    "vue": "^2.6.6",
    "vue-async-computed": "^3.6.1",
    "vue-directive-tooltip": "^1.5.1",
    "vue-mdl": "^1.1.1",
    "vue-notification": "^1.3.16",
    "vue-plugin-load-script": "^1.2.0",
    "vuetify-toast-snackbar": "^0.1.6",
    "web3": "^1.0.0-beta.55"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-eslint": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "babel-eslint": "^10.0.1",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "truffle-assertions": "^0.8.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
